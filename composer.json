{
    "name": "cron/cron-bundle",
    "type": "symfony-bundle",
    "description": "Symfony cron",
    "license": "MIT",
    "authors": [
        {
            "name": "Dries De Peuter",
            "email": "dries@nousefreak.be"
        }
    ],
    "require": {
        "php": ">=7.3",
        "symfony/framework-bundle": "^5.0",
        "cron/cron": "^1.4",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "~2.7"
    },
    "autoload": {
        "psr-4" : {
            "Cron\\CronBundle\\" : ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    }
}
